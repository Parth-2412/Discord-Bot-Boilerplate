{
  "name": "discord-bot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for creating a new discord bot (dynamic command handler and dynamic help command)",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc",
    "compile:watch": "tsc --watch --preserveWatchOutput",
    "dev": "nodemon --watch './src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "npm run compile",
    "start": "npm run compile && node ."
  },
  "author": "Parth2412",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/ws": "^7.4.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
